{
  "manifest_version": 3,
  "name": "Marketplace Messenger",
  "version": "1.0",
  "description": "Displays the marketplace dua before entering shopping websites for maximum rewards.",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}